<div class="container">
  <div class="jumbotron text-center">
    <h1>All Categories</h1>
  </div>
  <div id="content">
    <% @categories.each do |category| %>
      <%= image_tag "http://www.englishtearoom.com/wp-content/uploads/2015/06/heart-icon.png" %> <%= link_to category.name, category_path(category) %><br />
    <% end %>
  </div>
</div>
