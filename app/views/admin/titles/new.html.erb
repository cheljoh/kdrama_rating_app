<%= form_for([:admin, @title]) do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :image_path, "Image Path" %>
  <%= f.text_field :image_path %>
  <%= select("title", :category_id, Category.all.collect { |c| [c.name, c.id]}, { prompt:"select category"}) %>
  <%= f.submit %>
<% end %>
